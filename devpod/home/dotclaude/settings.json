{
  "workingDirectory": "/home/user/go-code",
  "env": {
    "BEAN_CODE_ENABLED": "1",
    "CLAUDE_AI_MODE": "1",
    "CLAUDE_CODE_USE_VERTEX": "1"
  },
  "terminal": {
    "shell": "/bin/zsh"
  },
  "statusLine": {
    "type": "script",
    "script": "/home/user/.claude/statusline-devpod.sh"
  },
  "mcpServers": {
    "phab": {
      "type": "stdio",
      "command": "aifx",
      "args": [
        "mcp",
        "run",
        "phab"
      ],
      "env": {}
    },
    "t3": {
      "type": "stdio",
      "command": "aifx",
      "args": [
        "mcp",
        "run",
        "t3"
      ],
      "env": {}
    },
    "code-reviewer": {
      "type": "stdio",
      "command": "aifx",
      "args": [
        "mcp",
        "run",
        "code-reviewer"
      ],
      "env": {}
    }
  }
}
